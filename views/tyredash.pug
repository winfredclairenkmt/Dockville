doctype html
head
  meta(charset='UTF-8')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  // Boxicons
  link(href='https://unpkg.com/boxicons@2.0.9/css/boxicons.min.css' rel='stylesheet')
  link(rel="stylesheet", href="/bootstrap-5.3.1-dist\css\bootstrap.css")
  // My CSS
  link(rel='stylesheet' href='/my_css/dash.css')
  title DockVille
// SIDEBAR
section#sidebar
  a.brand(href='./admindash')
    i.bx.bxs-car
    span.text DockVille
  ul.side-menu.top
    li.active
      a(href='./admindash')
        i.bx.bxs-dashboard
        span.text Dashboard
    li
      a(href='./cardash')
        i.bx.bxs-car
        span.text Parking-lot
    li
      a(href='./batterydash')
        i.bx.bxs-battery
        span.text battery-section
    li
      a(href='./tyredash')
        i.bx.bxs-gas-pump
        span.text Car Tyre Clinic
  ul.side-menu
    a.logout(href='/landing')
      i.bx.bxs-log-out-circle
      span.text Logout
// SIDEBAR
// CONTENT
section#content
  // NAVBAR  STARTS
  nav
    i.bx.bx-menu
    form(action='#')
      .form-input
        input(type='search' placeholder='Search...')
        button.search-btn(type='submit')
          i.bx.bx-search
    input#switch-mode(type='checkbox' hidden='')
    label.switch-mode(for='switch-mode')
    a.profile(href='#')
      img(src='img/people.png')
  // NAVBAR ENDS HERE
  // MAIN
  main
    .head-title
      .left
        h1 Dashboard
        ul.breadcrumb
          li
            a(href='#') Dashboard
          li
            i.bx.bx-chevron-right
          li
            a.active(href='#') Home
    ul.box-info
      li
        i.bx.bxs-gas-pump
        span.text
          h3 Car Tyre Clinic
          a.btn.btn-primary(type='' href="tyre") Add User
    .table-data
      .order
        .head
          h3 Parking-lot Activities
        //- table
        table
          
         
              
  // MAIN
// CONTENT
script(src='.js')
